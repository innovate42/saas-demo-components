  "dependencies": {
    "reactstrap": "^8.9.0"
  },
  "peerDependencies": {
    "@sentry/browser": "*",
    "react": "*"
  },
  "limioProps": [
    {
      "id": "logo",
      "label": "Logo",
      "type": "string",
      "format": "uri",
      "purpose": "image",
      "default": "https://sandbox.dev.limio.com/public/51ed4975-c2b9-47f3-bf20-70d7240564af/Limio-Original-White.png"
    },
    {
      "id": "logoHref",
