    {
      "id": "logoText",
      "label": "Logo Text",
      "type": "string",
      "default": ""
    },
    {
      "id": "items",
      "label": "Navigation bar items",
      "type": "list",
      "fields": {
        "label": {
          "id": "label",
          "label": "Text",
          "type": "string"
