        "button__limio_boolean": {
          "id": "button__limio_boolean",
          "label": "Show as button",
          "type": "boolean"
        },
        "login__limio_boolean": {
          "id": "button__limio_boolean",
          "label": "Show as login button",
          "type": "boolean"
        }
      },
      "default": [
        {
          "label": "Premium",
          "href": "/uk/premium",
          "button__limio_boolean": false,
          "login__limio_boolean": false
        },
        {
          "label": "Help",
          "href": "/help",
          "button__limio_boolean": false,
          "login__limio_boolean": false
        },
        {
          "label": "Download",
          "href": "/download",
          "button__limio_boolean": false,
          "login__limio_boolean": false
        }
      ]
    },
    {
      "id": "componentId",
      "label": "Component Id",
      "type": "string",
      "default": "nav-header-limio"
    }
  ],
  "translatableFields": [
    "Checkout",
    "My basket",
    "Quantity"
  ]
